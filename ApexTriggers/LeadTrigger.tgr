trigger LeadTrigger on Lead (before insert, before update) {
    if(Trigger.isBefore) {
        if(Trigger.isInsert) {
            LeadTriggerHelper.apexTrigger65A(Trigger.new, null);
        }
        
        if(Trigger.isUpdate) {
            LeadTriggerHelper.apexTrigger65A(Trigger.new, Trigger.oldMap);
        }
    }
}